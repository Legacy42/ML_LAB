   "source": [
    "import math\n",
    "data = [3,5,2,7,1,3]\n",
    "n = len(data)\n",
    "mean = sum(data) / n\n",
    "print(\"mean =\",mean)\n",
    "deviations = [(x - mean) ** 2 for x in data]\n",
    "variance = sum(deviations) / n\n",
    "std = math.sqrt(variance)\n",
    "print(\"std_deviation =\",std)\n",
    "st = [(x-mean)/std for x in data]\n",
    "print(\"standardisation =\",st)\n"
   ]
  }
